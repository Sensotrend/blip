<body>
	<div id="load">
		<div id='logocontainer'>
			<div class="logo">
				<img src="/img/blip.png" alt="" class="frame" />
				<img src="/img/ani-bg.png" alt="" class="ani-bg" />
			</div>
		</div>
	</div>
	<div id='tip'></div>
	<div id='content'>
		<header>	
			<div class='inner'>
				<div class='currentPatient'>
					<img src='/img/electra.png'>
					<h1>Electra White</h1>
				</div>
				<div class='container'>
